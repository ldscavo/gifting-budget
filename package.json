{
  "name": "gifting-budget",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "build": "cd client && vue-cli-service build",
    "lint": "cd client && vue-cli-service lint",
    "start:api": "node ./server",
    "dev:ui": "cd client && vue-cli-service serve",
    "dev:api": "nodemon ./server",
    "dev": "npm-run-all -p dev:api dev:ui",
    "migrate": "knex migrate:latest --env production",
    "rollback": "knex migrate:rollback"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "@saeris/vue-spinners": "^1.0.8",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "core-js": "^2.6.11",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.13",
    "lodash": "^4.17.21",
    "pg": "^7.18.2",
    "reinstall": "^2.0.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/essential"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
